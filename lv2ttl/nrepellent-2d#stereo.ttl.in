@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix param: <http://lv2plug.in/ns/ext/parameters#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix pprop: <http://lv2plug.in/ns/ext/port-props#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .

<https://github.com/lucianodato#me>
  a foaf:Person ;
  foaf:name "Luciano Dato" ;
  foaf:homepage <https://github.com/lucianodato> ;
  foaf:mbox <mailto:lucianodato@gmail.com> .

<https://github.com/lucianodato/noise-repellent#2d-stereo>
  a lv2:Plugin, lv2:SpectralPlugin, lv2:UtilityPlugin, doap:Project ;
  doap:maintainer <https://github.com/lucianodato#me> ;
  doap:name "Repelente de ruido 2D Stereo"@es ,
    "Répulseur de bruit 2D Stéréo"@fr ,
    "Noise repellent 2D Stereo" ;
  doap:license <https://opensource.org/licenses/LGPL-3.0> ;
  doap:shortdesc "Un plugin LV2 stereo para la reduccion de ruido usando NLM 2D"@es ,
    "Un greffon LV2 stéréo pour la réduction du bruit avec NLM 2D"@fr ,
    "A stereo LV2 plugin for noise reduction using 2D Non-Local Means" ;
  lv2:project <https://github.com/lucianodato/noise-repellent#2d-stereo> ;
  lv2:optionalFeature lv2:isLive, lv2:hardRTCapable, state:threadSafeRestore ;
  lv2:requiredFeature urid:map ;
  lv2:extensionData state:interface ;

  lv2:minorVersion @MINOR_VERSION@ ;
  lv2:microVersion @MICRO_VERSION@ ;

  lv2:port [
    a lv2:InputPort,
      lv2:ControlPort ;
    lv2:index 0 ;
    lv2:symbol "learn" ;
    lv2:name "Aprender ruido"@es ,
      "Apprendre le bruit"@fr ,
      "Learn noise" ;
    rdfs:comment "Captures a snapshot of the current noise to create a static profile. Effective when a noise-only segment is available." ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:default 0 ;
    lv2:portProperty lv2:toggled, lv2:integer ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 1 ;
    lv2:symbol "mode" ;
    lv2:name "Modo de reducción"@es ,
      "Mode de réduction"@fr ,
      "Reduction mode" ;
    rdfs:comment "Statistical method used for noise reduction." ;
    lv2:scalePoint [
            rdfs:label "Promedio (Equilibrado)"@es,
              "Moyenne (Équilibré)"@fr,
              "Average (Balanced)" ;
            rdf:value 1 ;
            rdfs:comment "Balanced reduction based on spectral averaging." ;
    ] ;
    lv2:scalePoint [
            rdfs:label "Mediana (Impulsivo)"@es,
             "Médiane (Impulsif)"@fr,
             "Median (Impulsive)" ;
            rdf:value 2 ;
            rdfs:comment "Effective against impulsive noise and clicks." ;
    ] ;
    lv2:scalePoint [
            rdfs:label "Máximo (Agresivo)"@es,
             "Maximum (Agressif)"@fr,
             "Maximum (Aggressive)" ;
            rdf:value 3 ;
            rdfs:comment "Aggressive reduction targeting noise peaks." ;
    ] ;
    lv2:minimum 1 ;
    lv2:maximum 3 ;
    lv2:default 1 ;
    lv2:portProperty lv2:integer ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 2 ;
    lv2:symbol "adaptive_noise" ;
    lv2:name "Ruido adaptativo"@es ,
      "Bruit adaptatif"@fr ,
      "Adaptive noise" ;
    rdfs:comment "Automatically tracks noise changes in real-time. Can be used alone or to refine a captured profile." ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:default 0 ;
    lv2:portProperty lv2:toggled, lv2:integer ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 3 ;
    lv2:symbol "adaptive_method" ;
    lv2:name "Metodo adaptativo"@es ,
      "Méthode adaptative"@fr ,
      "Adaptive method" ;
    lv2:scalePoint [
            rdfs:label "SPP-MMSE (Speech)",
              "SPP-MMSE (Voz)"@es,
              "SPP-MMSE (Voix)"@fr;
            rdf:value 0;
            rdfs:comment "Best for speech/voice and podcasts. Very fast adaptation to changing noise. Can be aggressive with music due to speech-optimized detection."
    ] ;
    lv2:scalePoint [
            rdfs:label "Brandt (Music)",
              "Brandt (Música)"@es,
              "Brandt (Musique)"@fr;
            rdf:value 1;
            rdfs:comment "Best for music with hiss/broadband noise (vinyl, tape). Not suitable for tonal/harmonic noise (hum, buzz)."
    ] ;
    lv2:scalePoint [
            rdfs:label "Min. Statistics (General)",
              "Min. Statistics (General)"@es,
              "Min. Statistics (Général)"@fr;
            rdf:value 2;
            rdfs:comment "Robust and stable for music and general use. Works with any noise type. Slower adaptation than SPP-MMSE but more reliable."
    ] ;
    lv2:minimum 0 ;
    lv2:maximum 2 ;
    lv2:default 2 ;
    lv2:portProperty lv2:integer ;
    rdfs:comment "Selects the adaptive noise estimation algorithm. SPP-MMSE: very fast, best for speech/podcasts but can be aggressive with music. Brandt: robust for music with hiss/broadband noise (vinyl, tape) but NOT tonal noise (hum, buzz). Min. Statistics (Martin): stable and reliable for music and general use, works with any noise type." ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 4 ;
    lv2:symbol "reduction" ;
    lv2:name "Reducción"@es ,
      "Réduction"@fr ,
      "Reduction" ;
    lv2:minimum 0.0 ;
    lv2:maximum 40.0 ;
    lv2:default 15.0 ;
    lv2:designation lv2:threshold ;
    units:unit units:db ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 5 ;
    lv2:symbol "masking_transparency" ;
    lv2:name "Transparencia de enmascaramiento"@es ,
      "Transparence du masquage"@fr ,
      "Masking transparency" ;
    rdfs:comment "Balances transient preservation vs. noise floor silence. 0%: Maximum reduction, 50%: Natural sweet spot (recommended), 100%: Pure transparency." ;
    lv2:minimum 0.0 ;
    lv2:maximum 100.0 ;
    lv2:default 50.0 ;
    units:unit units:pc ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 7 ;
    lv2:symbol "nlm_smoothing" ;
    lv2:name "Suavizado"@es ,
      "Lissage"@fr ,
      "Smoothing" ;
    lv2:minimum 0.1 ;
    lv2:maximum 10.0 ;
    lv2:default 5.0 ;
    rdfs:comment "Controls structural similarity matching (transient preservation). Higher values = more smoothing of the noise floor." ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 8 ;
    lv2:symbol "whitening" ;
    lv2:name "Blanqueo de residuo"@es ,
      "Blanchissement du bruit"@fr ,
      "Residual whitening" ;
    lv2:minimum 0.0 ;
    lv2:maximum 100.0 ;
    lv2:default 0.0 ;
    units:unit units:pc ;
  ], [
    a lv2:InputPort,
    lv2:ControlPort ;
    lv2:index 9 ;
    lv2:symbol "Residual_listen" ;
    lv2:name "Escuchar Residuo"@es ,
      "Écoute résiduelle"@fr ,
      "Residual listen" ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:default 0 ;
    lv2:portProperty lv2:toggled, lv2:integer ;
  ], [
    a lv2:InputPort,
      lv2:ControlPort ;
    lv2:index 10 ;
    lv2:symbol "bypass" ;
    lv2:name "Bypass"@es ,
      "Bypass"@fr ,
      "Bypass" ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:default 0 ;
    lv2:portProperty lv2:toggled, lv2:integer ;
  ], [
    a lv2:InputPort,
      lv2:ControlPort ;
    lv2:index 11 ;
    lv2:symbol "reset" ;
    lv2:name "Reiniciar perfil"@es ,
      "Réinitialiser le perfil"@fr ,
      "Reset profile" ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:default 0 ;
    lv2:portProperty lv2:toggled, lv2:integer ;
  ], [
    a lv2:OutputPort,
      lv2:ControlPort ;
    lv2:name "latency" ;
    lv2:index 12 ;
    lv2:symbol "latency" ;
    lv2:minimum 0 ;
    lv2:maximum 8192 ;
    lv2:designation lv2:latency ;
    lv2:portProperty lv2:integer ;
    units:unit units:frame ;
  ], [
    a lv2:AudioPort,
      lv2:InputPort ;
    lv2:index 13 ;
    lv2:symbol "input_l" ;
    lv2:name "Input L" ;
    lv2:designation pg:left ;
  ], [
    a lv2:AudioPort,
      lv2:OutputPort ;
    lv2:index 14 ;
    lv2:symbol "output_l" ;
    lv2:name "Output L" ;
    lv2:designation pg:left ;
  ], [
    a lv2:AudioPort,
      lv2:InputPort ;
    lv2:index 15 ;
    lv2:symbol "input_r" ;
    lv2:name "Input R" ;
    lv2:designation pg:right ;
  ], [
    a lv2:AudioPort,
      lv2:OutputPort ;
    lv2:index 16 ;
    lv2:symbol "output_r" ;
    lv2:name "Output R" ;
    lv2:designation pg:right ;
  ];
  rdfs:comment "Un plugin LV2 stereo para la reduccion de ruido usando filtrado NLM 2D"@es,
               "Un greffon LV2 stéréo pour la réduction du bruit avec filtrage NLM 2D"@fr,
               "A stereo LV2 plugin for broadband noise reduction using 2D Non-Local Means filtering" ;
.
