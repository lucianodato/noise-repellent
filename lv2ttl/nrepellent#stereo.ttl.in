@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix param: <http://lv2plug.in/ns/ext/parameters#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix pprop: <http://lv2plug.in/ns/ext/port-props#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .

<https://github.com/lucianodato#me>
  a foaf:Person ;
  foaf:name "Luciano Dato" ;
  foaf:homepage <https://github.com/lucianodato> ;
  foaf:mbox <mailto:lucianodato@gmail.com> .

<https://github.com/lucianodato/noise-repellent-stereo#new>
  a lv2:Plugin, lv2:SpectralPlugin, lv2:UtilityPlugin, doap:Project ;
  doap:maintainer <https://github.com/lucianodato#me> ;
  doap:license <https://opensource.org/licenses/LGPL-3.0> ;
  doap:name "Repelente de ruido"@es ,
    "Répulseur de bruit"@fr ,
    "Noise repellent" ;
  doap:shortdesc "Un plugin LV2 para la reduccion de ruido estereo de banda ancha"@es ,
               "Un greffon LV2 pour la reducción du bruit stéréo à large bande"@fr ,
               "An LV2 plugin optimized for stereo broadband and stationary noise reduction" ;
  lv2:project <https://github.com/lucianodato/noise-repellent-stereo#new> ;
  lv2:optionalFeature lv2:isLive, lv2:hardRTCapable ;
  lv2:extensionData state:interface ;
  lv2:requiredFeature urid:map ;

  lv2:minorVersion @MINOR_VERSION@ ;
  lv2:microVersion @MICRO_VERSION@ ;

  lv2:port [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 0 ;
    lv2:symbol "noise_learn" ;
    lv2:name "Aprender perfil de ruido"@es ,
      "Apprendre le perfil du bruit"@fr ,
      "Learn noise profile" ;
    rdfs:comment "Captures a snapshot of the current noise to create a static profile. Effective when a noise-only segment is available." ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:default 0 ;
    lv2:portProperty lv2:toggled, lv2:integer ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 1 ;
    lv2:symbol "steering_response" ;
    lv2:name "Respuesta de dirección"@es,
      "Réponse du pilotage"@fr,
      "Steering response" ;
    rdfs:comment "Sets the bias for Intelligent Steering. -100%: Conservative/Median, +100%: Aggressive/Max." ;
    lv2:minimum -100.0 ;
    lv2:maximum 100.0 ;
    lv2:default 0.0 ;
    units:unit units:pc ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 2 ;
    lv2:symbol "reset_noise_profile" ;
    lv2:name "Reiniciar perfil de ruido"@es ,
      "Réinitialiser le profil de bruit"@fr ,
      "Reset noise profile" ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:default 0 ;
    lv2:portProperty lv2:toggled, lv2:integer, pprop:trigger;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 3 ;
    lv2:symbol "adaptive_noise" ;
    lv2:name "Ruido adaptativo"@es ,
      "Bruit adaptatif"@fr ,
      "Adaptive noise" ;
    rdfs:comment "Automatically tracks noise changes in real-time. Can be used alone or to refine a captured profile." ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:default 0 ;
    lv2:portProperty lv2:toggled, lv2:integer ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 4 ;
    lv2:symbol "adaptive_method" ;
    lv2:name "Metodo adaptativo"@es ,
      "Méthode adaptative"@fr ,
      "Adaptive method" ;
    lv2:scalePoint [
            rdfs:label "SPP-MMSE (Speech)",
              "SPP-MMSE (Voz)"@es,
              "SPP-MMSE (Voix)"@fr;
            rdf:value 0;
            rdfs:comment "Best for speech/voice and podcasts. Very fast adaptation to changing noise. Can be aggressive with music due to speech-optimized detection."
    ] ;
    lv2:scalePoint [
            rdfs:label "Brandt (Music)",
              "Brandt (Música)"@es,
              "Brandt (Musique)"@fr;
            rdf:value 1;
            rdfs:comment "Best for music with hiss/broadband noise (vinyl, tape). Not suitable for tonal/harmonic noise (hum, buzz)."
    ] ;
    lv2:scalePoint [
            rdfs:label "Min. Statistics (General)",
              "Min. Statistics (General)"@es,
              "Min. Statistics (Général)"@fr;
            rdf:value 2;
            rdfs:comment "Robust and stable for music and general use. Works with any noise type. Slower adaptation than SPP-MMSE but more reliable."
    ] ;
    lv2:minimum 0 ;
    lv2:maximum 2 ;
    lv2:default 2 ;
    lv2:portProperty lv2:integer ;
    rdfs:comment "Selects the adaptive noise estimation algorithm. SPP-MMSE: very fast, best for speech/podcasts but can be aggressive with music. Brandt: robust for music with hiss/broadband noise (vinyl, tape) but NOT tonal noise (hum, buzz). Min. Statistics (Martin): stable and reliable for music and general use, works with any noise type." ;
  ], [    
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 5 ;
    lv2:symbol "reduction" ;
    lv2:name "Cantidad de reduccion"@es ,
      "Quantité de reducción"@fr ,
      "Reduction amount" ;
    lv2:minimum 0.0 ;
    lv2:maximum 40.0 ;
    lv2:default 15.0 ;
    lv2:designation lv2:threshold ;
    units:unit units:db ;
    units:conversion [
			units:to units:coef;
		];
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 6 ;
    lv2:symbol "tonal_reduction" ;
    lv2:name "Reducción tonal"@es,
      "Réduction tonale"@fr,
      "Tonal reduction" ;
    rdfs:comment "Independent reduction amount for detected tonal components (hum, resonance)." ;
    lv2:minimum 0.0 ;
    lv2:maximum 48.0 ;
    lv2:default 30.0 ;
    units:unit units:db ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 7 ;
    lv2:symbol "suppression" ;
    lv2:name "Supresión"@es ,
      "Suppression"@fr ,
      "Suppression" ;
    rdfs:comment "Controls how aggressively the algorithm hunts for musical noise artifacts. High values (100%) use max oversubtraction for a 'surgical' clean but can cause more ducking. Combined with Masking Transparency (Veto) for optimal protection." ;
    lv2:minimum 0.0 ;
    lv2:maximum 100.0 ;
    lv2:default 20.0 ;
    units:unit units:pc ;
  ], [
    a lv2:ControlPort,
    lv2:InputPort ;
    lv2:index 8 ;
    lv2:symbol "smoothing" ;
    lv2:name "Suavizado"@es ,
      "Lissage"@fr ,
      "Smoothing" ;
    lv2:minimum 0.0 ;
    lv2:maximum 100.0 ;
    lv2:default 0.0 ;
    units:unit units:pc ;
  ], [
    a lv2:ControlPort,
      lv2:InputPort ;
    lv2:index 9 ;
    lv2:symbol "masking_transparency" ;
    lv2:name "Transparencia de mascara"@es,
      "Transparence du masquage"@fr,
      "Masking transparency" ;
    rdfs:comment "Balances between noise reduction and transient preservation. High transparency preserves attacks and delicate details but can leave more noise. Standard is 'Natural' (50%)." ;
    lv2:minimum 0.0 ;
    lv2:maximum 100.0 ;
    lv2:default 50.0 ;
    units:unit units:pc ;
  ], [
    a lv2:ControlPort,
    lv2:InputPort ;
    lv2:index 10 ;
    lv2:symbol "whitening" ;
    lv2:name "Blanqueo de residuo"@es ,
      "Blanchissement du bruit"@fr ,
      "Residual whitening" ;
    lv2:minimum 0.0 ;
    lv2:maximum 100.0 ;
    lv2:default 0.0 ;
    units:unit units:pc ;
  ], [
    a lv2:InputPort,
    lv2:ControlPort ;
    lv2:index 11 ;
    lv2:symbol "Residual_listen" ;
    lv2:name "Escuchar Residuo"@es ,
      "Écoute résiduelle"@fr ,
      "Residual listen" ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:default 0 ;
    lv2:portProperty lv2:toggled, lv2:integer ;
  ], [
    a lv2:InputPort,
      lv2:ControlPort ;
    lv2:index 12 ;
    lv2:symbol "bypass" ;
    lv2:name "Bypass"@es ,
      "Bypass"@fr ,
      "Bypass" ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:default 0 ;
    lv2:portProperty lv2:toggled, lv2:integer ;
  ], [
    a lv2:OutputPort,
      lv2:ControlPort ;
    lv2:name "latency" ;
    lv2:index 13 ;
    lv2:symbol "latency" ;
    lv2:minimum 0 ;
    lv2:maximum 8192 ;
    lv2:designation lv2:latency ;
    lv2:portProperty lv2:integer ;
    units:unit units:frame ;
  ], [
    a lv2:AudioPort,
      lv2:InputPort ;
    lv2:index 14 ;
    lv2:symbol "input_1" ;
    lv2:name "Input L" ;
    pg:group <https://github.com/lucianodato/noise-repellent-stereo#new/InGroup> ;
    lv2:designation pg:left ;
  ], [
    a lv2:AudioPort,
      lv2:OutputPort ;
    lv2:index 15 ;
    lv2:symbol "output_1" ;
    lv2:name "Output L" ;
    pg:group <https://github.com/lucianodato/noise-repellent-stereo#new/OutGroup> ;
    lv2:designation pg:left ;
  ], [
    a lv2:AudioPort,
      lv2:InputPort ;
    lv2:index 16 ;
    lv2:symbol "input_2" ;
    lv2:name "Input R" ;
    pg:group <https://github.com/lucianodato/noise-repellent-stereo#new/InGroup> ;
    lv2:designation pg:right ;
  ], [
    a lv2:AudioPort,
      lv2:OutputPort ;
    lv2:index 17 ;
    lv2:symbol "output_2" ;
    lv2:name "Output R" ;
    pg:group <https://github.com/lucianodato/noise-repellent-stereo#new/OutGroup> ;
    lv2:designation pg:right ;
  ];
  rdfs:comment "Un plugin LV2 para la reduccion de ruido estereo"@es,
               "Un greffon LV2 pour la reducción du bruit à large bande"@fr,
               "An LV2 plugin optimized for stereo broadband and stationary noise reduction" ;
.
